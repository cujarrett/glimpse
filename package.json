{
  "name": "glimpse",
  "version": "0.30.3",
  "author": "Matt Jarrett",
  "license": "MIT",
  "private": true,
  "description": "A React JS web app to show GitHub user contribution metrics",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cujarrett/glimpse.git"
  },
  "keywords": [
    "React",
    "GitHub",
    "Metrics"
  ],
  "scripts": {
    "commit": "git-cz",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false ----collectCoverage=true --detectOpenHandles",
    "lint": "eslint .",
    "fix-lint": "eslint . --fix",
    "ci": "npm run lint && npm run test",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "contributors": [
    {
      "name": "Matt Jarrett",
      "web": "https://mattjarrett.dev"
    }
  ],
  "homepage": ".",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "~4.12.2",
    "@material-ui/icons": "~4.11.2",
    "babel-polyfill": "~6.26.0",
    "font-awesome": "~4.7.0",
    "node-fetch": "~2.6.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-scripts": "~4.0.3",
    "react-share": "~4.4.0",
    "recharts": "~2.0.10",
    "web-vitals": "~2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~13.1.0",
    "@commitlint/config-conventional": "~13.1.0",
    "@semantic-release/changelog": "~5.0.1",
    "@semantic-release/commit-analyzer": "~8.0.1",
    "@semantic-release/exec": "~5.0.0",
    "@semantic-release/git": "~9.0.0",
    "@semantic-release/github": "~7.2.3",
    "@semantic-release/npm": "~7.1.3",
    "@semantic-release/release-notes-generator": "~9.0.3",
    "@testing-library/jest-dom": "~5.14.1",
    "@testing-library/react": "~12.0.0",
    "@testing-library/user-event": "~13.2.0",
    "commitizen": "~4.2.4",
    "cz-conventional-changelog": "~3.3.0",
    "husky": "~7.0.1",
    "jest-environment-jsdom-sixteen": "~2.0.0",
    "semantic-release": "~17.4.4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
